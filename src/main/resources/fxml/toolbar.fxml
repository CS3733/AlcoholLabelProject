            <Button fx:id="extraButton" mnemonicParsing="false" onMouseClicked="#extraFunction" prefHeight="51.0" prefWidth="132.0" style="-fx-background-color: -swatch-green; -fx-font-size: 18;" text="PROFILE">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Button>
            <Button fx:id="utility" mnemonicParsing="false" onMouseClicked="#utilityButton" prefHeight="50.0" prefWidth="193.0" style="-fx-background-color: -swatch-green; -fx-font-size: 18;" text="MY APPLICATIONS">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Button>
            <Button fx:id="logButton" mnemonicParsing="false" onMouseClicked="#loadLog" prefWidth="96.0" style="-fx-background-color: -swatch-green; -fx-font-size: 18;" text="LOGIN" />
