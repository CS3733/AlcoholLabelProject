<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="96.0" prefWidth="1024.0" style="-fx-background-color: #319b54;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.emeraldElves.alcohollabelproject.UserInterface.ToolbarController">
   <children>
      <Label layoutX="65.0" layoutY="33.0" onMouseClicked="#goHome" text="EMERALD ELVES COLA" />
      <Button fx:id="logButton" layoutX="896.0" layoutY="29.0" mnemonicParsing="false" onMouseClicked="#loadLog" prefWidth="96.0" style="-fx-background-color: -swatch-green; -fx-font-size: 18;" text="LOGIN" />
      <Button fx:id="utility" layoutX="703.0" layoutY="32.0" mnemonicParsing="false" onMouseClicked="#utilityButton" prefHeight="32.0" prefWidth="193.0" style="-fx-background-color: -swatch-green; -fx-font-size: 18;" text="MY APPLICATIONS">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </Button>
      <Button fx:id="extraButton" layoutX="571.0" layoutY="33.0" mnemonicParsing="false" onMouseClicked="#extraFunction" prefHeight="32.0" prefWidth="132.0" style="-fx-background-color: -swatch-green; -fx-font-size: 18;" text="PROFILE">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </Button>
      <ImageView fx:id="backBtn" fitHeight="32.0" fitWidth="32.0" layoutX="14.0" layoutY="34.0" onMouseClicked="#goBack" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/arrow-left.png" />
         </image>
      </ImageView>
   </children>
   <styleClass>
      <String fx:value="tool-bar" />
      <String fx:value="toolbar-colored" />
   </styleClass>
</Pane>
