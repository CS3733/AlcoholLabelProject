      <Label layoutX="31.0" layoutY="-1.0" onMouseClicked="#goHome" prefHeight="96.0" prefWidth="263.0" text="EMERALD ELVES COLA" />
      <HBox layoutX="550.0" layoutY="26.0" prefHeight="40.0" prefWidth="460.0">
